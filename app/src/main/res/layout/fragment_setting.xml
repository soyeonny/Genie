                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="#21202B"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/first_iv"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerVertical="true"
                            android:layout_margin="10dp"
                            android:clickable="false"
                            android:focusable="false"
                            android:src="@drawable/ic_favorite" />

                        <TextView
                            android:id="@+id/first_tv"
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_toRightOf="@+id/first_iv"
                            android:clickable="false"
                            android:focusable="false"
                            android:gravity="center_vertical"
                            android:text="건강 데이터 연동하기"
                            android:textColor="#ffff"
                            android:textSize="20dp" />

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="50dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_toEndOf="@+id/first_tv"
                            android:clickable="true"
                            android:gravity="center_vertical"
                            android:text="ON >"
                            android:textColor="#807E7E"
                            android:textSize="20dp" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/sec_cv"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/first_cv"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">

